@{
    ViewBag.Title = "Home Page";
    Layout = null;
}

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Машины</title>
</head>
<body>
    <div>
        
        <table>
            <tr>
                <td><p>Название книги</p></td>
                <td><p>Автор</p></td>
                <td><p>Цена</p></td>
                <td></td>
            </tr>
            @foreach (var b in ViewBag.brand)
            {
                <tr>
                    <td><p>@b.Name</p></td>
                    <td><a href="/Home/Models/@b.Id">Выбрать</a></td>
                    
                </tr>
               
            }
        </table>
    </div>
</body>
</html>